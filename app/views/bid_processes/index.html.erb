<center>
<div class="container">
  <div class="row">
    <div class="col-sm-12"><h1>Bid Processes</h1></div>
  </div>

<table class="table table-bordered" width="50">
  <thead>
    <tr>
      <th>Bid</th>
      <th>Produce</th>
      <th>BidPro</th>
      <th>Bidder</th>
      <th>Price (RM)</th>
      <th>Bidding time</th>
    </tr>
  </thead>

  <tbody>
    <% @bid_processes.each do |bid_process| %>
      <tr>
        <td><%= bid_process.bid.id %></td>
        <td><%= bid_process.bid.produce.producename %></td>
        <td><%= bid_process.id %></td>
        <td><%= bid_process.bidder.name %></td>
        <td><%= bid_process.price %></td>
        <td><%= bid_process.created_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>


<%= link_to 'Back', bids_path %>
<br>
</div>
</center>
