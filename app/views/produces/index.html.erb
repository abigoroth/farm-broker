<p id="notice"><%= notice %></p>

<h1>Produces</h1>

<table>
  <thead>
    <tr>
      <th>Producename</th>
      <th>Producetype</th>
      <th>Producedate</th>
      <th>Producequantity</th>
      <th>Producedescription</th>
      <th>Farmsiteid</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @produces.each do |produce| %>
      <tr>
        <td><%= produce.producename %></td>
        <td><%= produce.producetype %></td>
        <td><%= produce.producedate %></td>
        <td><%= produce.producequantity %></td>
        <td><%= produce.producedescription %></td>
        <td><%= produce.farmsite_id %></td>
        <td><%= link_to 'Show', produce %></td>
        <td><%= link_to 'Edit', edit_produce_path(produce) %></td>
        <td><%= link_to 'Destroy', produce, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Produce', new_produce_path %>
