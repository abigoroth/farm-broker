<%= simple_form_for(@farmsite) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :size %>
    <%= f.input :address %>
    <%= f.input :ownername %>
    <%= f.input :ownerphoneno %>
    <%= f.input :district_id %>
    <%= f.input :broker_id %>

  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>

