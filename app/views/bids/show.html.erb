<center>
  <p id="notice"><%= notice %></p>

  <div class="row">
    <div class="col-sm-4"></div>

    <div class="col-sm-4">
      <div class="well">
        <div class="panel panel-default">
          <div class="panel-body">

            <p><strong>Produce:</strong><%= @bid.produce.producename  %></p>

            <p><strong>Start time:</strong><%= @bid.start_time %></p>

            <p><strong>End time:</strong><%= @bid.end_time %></p>

            <p><strong>Start price: RM</strong><%= @bid.start_price %></p>

            <p><strong>Last price: RM</strong><%= @bid.max_price %></p>


            <%= link_to 'Edit', edit_bid_path(@bid), class: "btn btn-default" %> 
            <%= link_to 'Back', bids_path, class: "btn btn-default" %>


          </div>
        </div>
      <div class="col-sm-1"></div>
    </div>

</center>
